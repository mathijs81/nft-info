{
  "name": "fastify-typescript-starter",
  "version": "4.1.0",
  "description": "Fastify TypeScript starter repository",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "vercel-build": "rimraf vercel-dist;cd ..;rimraf dist;npm i;npm run build;cp -a dist backend-worker/vercel-dist;cd backend-worker",
    "start": "node dist/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@tableland/sdk": "^0.0.1",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.0",
    "fastify": "^3.27.4",
    "nft.storage": "^6.0.2",
    "node-fetch": "^3.2.2"
  },
  "devDependencies": {
    "@tableland/cli": "^0.0.1",
    "@types/node": "latest",
    "@vercel/node": "^1.13.0",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2"
  }
}
